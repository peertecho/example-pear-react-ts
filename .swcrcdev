[
  {
    "test": ".*\\.ts$",
    "jsc": {
      "baseUrl":"./",
      "paths": {
         "@/*": ["src/*"]
      },
      "parser": {
        "syntax": "typescript"
      }
    }
  },
  {
    "test": ".*\\.tsx$",
    "jsc": {
      "baseUrl":"./",
      "paths": {
         "@/*": ["src/*"]
      },
      "parser": {
        "syntax": "typescript",
        "jsx": true
      },
      "transform": {
        "react": {
          "runtime": "automatic",
          "development": true,
          "refresh": true
        }
      }
    }
  }
]
